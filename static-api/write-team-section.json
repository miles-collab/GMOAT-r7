{
  "openapi": "3.1.0",
  "info": {
    "title": "Team Section Writer",
    "version": "1.0.0",
    "description": "Generates a full-length Team section based on extracted bios, notes, and team profiles."
  },
  "servers": [
    {
      "url": "https://miles-collab.github.io/GMOAT-r7/static-api"
    }
  ],
  "paths": {
    "/write-team-section.json": {
      "get": {
        "operationId": "writeTeamSection",
        "summary": "Generate a Team Section",
        "description": "Outputs team bios, structure, friction points, and hiring roadmap with bold formatting.",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "teamSection": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
